<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#C0C0C0">


    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="109dp"
        android:layout_height="82dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:contentDescription="@string/desc"
        android:src="@drawable/pertamina_logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/pertamina_logo" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="106dp"
        android:layout_height="77dp"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="8dp"
        android:contentDescription="@string/desc"
        android:src="@drawable/elpiji_logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/elpiji_logo" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="341dp"
        android:layout_height="31dp"
        android:layout_marginBottom="250dp"
        android:layout_marginEnd="125dp"
        android:layout_marginStart="125dp"
        android:layout_marginTop="30dp"
        android:text="@string/text"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.283" />

    <EditText
        android:id="@+id/etSPPBE"
        android:layout_width="wrap_content"
        android:layout_height="23dp"
        android:layout_marginBottom="154dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        android:background="#A9A9A9"
        android:ems="10"
        android:hint="@string/sppbe_hint"
        android:inputType="textPersonName"
        android:text=""
        android:textColor="#808080"
        android:textColorHint="#808080"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView6"
        app:layout_constraintHorizontal_bias="0.53"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_constraintVertical_bias="0.053" />

    <EditText
        android:id="@+id/etPlat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        android:background="#A9A9A9"
        android:ems="10"
        android:hint="@string/plat_hint"
        android:inputType="textPersonName"
        android:text=""
        android:textColor="#808080"
        android:textColorHint="#808080"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/etSPPBE"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/etSPPBE"
        app:layout_constraintTop_toBottomOf="@+id/etSPPBE"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btSave"
        android:layout_width="217dp"
        android:layout_height="36dp"
        android:layout_marginBottom="43dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/roundedbtngreen"
        android:text="@string/save_button"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/etPlat"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/etPlat"
        app:layout_constraintTop_toBottomOf="@+id/etAlamat"
        app:layout_constraintVertical_bias="0.396" />

    <EditText
        android:id="@+id/etKapasitas"
        android:layout_width="155dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="244dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        android:background="#A9A9A9"
        android:ems="10"
        android:hint="KAPASITAS"
        android:inputType="number"
        android:textColor="#808080"
        android:textColorHint="#808080"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/etPlat"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/etPlat"
        app:layout_constraintTop_toBottomOf="@+id/etPlat"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="8dp"
        android:text="MT"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/etKapasitas"
        app:layout_constraintStart_toEndOf="@+id/etKapasitas"
        app:layout_constraintTop_toTopOf="@+id/etKapasitas" />

    <EditText
        android:id="@+id/etAlamat"
        android:layout_width="wrap_content"
        android:layout_height="93dp"
        android:layout_marginBottom="152dp"
        android:layout_marginTop="8dp"
        android:background="#A9A9A9"
        android:ems="10"
        android:hint="ALAMAT"
        android:inputType="textMultiLine"
        android:textColor="#808080"
        android:textColorHint="#808080"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/etPlat"
        app:layout_constraintStart_toStartOf="@+id/editText"
        app:layout_constraintTop_toBottomOf="@+id/etKapasitas"
        app:layout_constraintVertical_bias="0.0" />

</android.support.constraint.ConstraintLayout>